{
  "expo": {
    "name": "Info Source News",
    "slug": "info-source",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconNew.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-iconNew.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hasan_dev.infosource"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3a595ebf-fe5e-4055-a894-72f225f1423b"
      }
    }
  }
}
